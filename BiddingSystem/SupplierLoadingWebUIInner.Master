    <script type="text/javascript">
     var DataListCategorySet = <%= getJsonReceived() %>
     LoadCategories();
     
    function LoadCategories()
    {
        var text = "";
        var field = "";
        var ListId = "";
        var htmlcode  = 
         '<option value="" id='+0+'>All Category</option>' + ''
         text += htmlcode;
        for (var i = 1; i <= DataListCategorySet.length; i++) {
            field = DataListCategorySet[i - 1].split('-');
            ListId = field[0]+"_"+field[1];
            htmlcode =
                  '<option value="" id='+ListId+'>'+field[1]+'</option>' ;
            text += htmlcode;
        }
        document.getElementById("agileinfo-nav_search").innerHTML = text;
     } 
    </script>
